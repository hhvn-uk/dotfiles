super + Return
	st

super + t
	pkill -SIGUSR1 herbe

super + shift + t
	pkill -SIGUSR2 herbe

super + ctrl + t
	pkill -SIGKILL herbe; herbe

super + alt + t
	herbe "test"

super + a
	addrbook

super + shift + a
	mailselect

super + b
	tabbed -k -c surf -e > /tmp/tabbed.xid

super + shift + n
	st -c mpvrcp -g 150x30 -b -e mpvrcp /tmp/mpv-socket

super + n 
	st -t neomutt -c neomutt -e sh -c "neomutt; $SHELL"

super + r
	st -t sfeed -e sh -c 'SFEED_PLUMBER=plumb sfeed_curses $HOME/.local/sfeed/*'

super + c
	st -e slock_mod /tmp/mpv-socket /tmp/mpv-socket2 /tmp/mpv-socket3

super + shift + c
	slock

super + d
	dmenu_run

super + u
	plumb $(xclip -o)

super + e
	st -e tmux -f $HOME/.config/tmux/config

super + s
	dpass

super + shift + s
	maim -s | hlpaste "png"

super + ctrl + s
	maim | hlpaste "png"

super + w
	wallp $HOME/general/images/wallpapers/

super + ctrl + w
	wallblur

super + shift + g
	chradio

super + o
	st -t torrents -c torrents -e sh -c "torrents; $SHELL"

super + shift + o
	addtorrent xclip

super + alt + t
	hmpv quit /tmp/mpv-socket3

super + ctrl + y
	dmpv general/videos/ /tmp/mpv-socket2 /tmp/mpv-socket --fs ls

super + ctrl + u
	dmpv general/music/ /tmp/mpv-socket /tmp/mpv-socket2 --no-video

super + alt + {y,u}
	rmpv general/{videos,music}/list 150

super + ctrl + {i,p}
	hmpv {back,forward} /tmp/mpv-socket

super + alt + {i,p}
	hmpv {prev,next} /tmp/mpv-socket

super + {ctrl,alt} + o
	hmpv {toggle,quit} /tmp/mpv-socket

super + {minus,equal}
	hvol 3%{-,+}

super + shift {minus,equal}
	hvol 6%{-,+}

{XF86AudioLowerVolume, XF86AudioRaiseVolume}
	hvol 3%{-,+}

shift + {XF86AudioLowerVolume, XF86AudioRaiseVolume}
	hvol 6%{-,+}

super + y
	st -e sh -c 'autowifi && cu undock'

super + shift + y
	st -e sh -c 'wifioff && cu dock'

super + ctrl + {j,k}
	for void in $(seq 3); do xdotool click {5,4}; done

super + shift + h
	st -e htop

Menu
	xdotool click 2

{KP_Begin,KP_Left,KP_Right,KP_Down,KP_End,KP_Next,KP_Insert,KP_Delete,KP_Enter,KP_Prior,KP_Add,KP_Up,KP_Subtract,KP_Multiply,KP_Divide}
	st -e slock_mod /tmp/mpv-socket /tmp/mpv-socket2 /tmp/mpv-socket3

#thinkpad codes

# XF86Sleep
# XF86Batter
XF86ScreenSaver
	st -e slock_mod

XF86Display
	xset dpms force off

# XF86TouchpadToggle
# XF86Suspend
