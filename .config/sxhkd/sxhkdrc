super + {, shift, ctrl, alt, shift + alt} + v
	dinput "Are you sure?" "{halt zzz, halt halt, halt reboot, halt ZZZ, pkill dwm}"

super + Return
	st

super + t
	pkill -SIGUSR1 herbe

super + shift + t
	pkill -SIGUSR2 herbe

super + ctrl + t
	pkill -SIGKILL herbe; herbe

super + alt + t
	herbe "test"

super + a
	addrbook

super + shift + a
	mailselect

super + b
	tabbed -c surf -e > /tmp/tabbed.xid

super + shift + b
	dmpvurl /tmp/mpv-socket /tmp/mpv-socket2

super + n
	st -c mpvrcp -e mpvrcp /tmp/mpv-socket

super + shift + n 
	st -e neomutt

super + r
	st -e sh -c 'sfeed_update $HOME/.config/sfeed/config && SFEED_PLUMBER=urlhand2 sfeed_curses $HOME/.cache/sfeed/*'

super + x
	sxhkdrestart; herbe "Restarted sxhkd" && st -e nvim $XDG_CONFIG_HOME/sxhkd/sxhkdrc

super + ctrl + b
	queuevid

super + c
	st -e slock_mod /tmp/mpv-socket /tmp/mpv-socket2 /tmp/mpv-socket3

super + shift + c
	slock

super + d
	dmenu_run

super + e
	st -e tmux -f $HOME/.config/tmux/config

super + ctrl + e
	rfeh ~/images/wallpapers/ auto right

super + m
	dmon

super + ctrl + q
	rfeh ~/images/wallpapers/ auto left

super + s
	dpass

super + shift + s
	maim -s | hlpaste "png"

super + ctrl + s
	maim | hlpaste "png"

super + w
	rfeh ~/images/wallpapers/

super + ctrl + w
	wallblur

super + alt + w
	fehffile ~/.cache/rfeh/prev

super + shift + w
	colour-wall

super + shift + g
	chradio

super + ctrl + g
	st -e sh -c 'nvim ~/.local/notes'

super + g
	st -e sh -c 'rlwrap cgo'

super + o
	st -e torrents

super + shift + o
	addtorrent xclip

super + alt + t
	hmpv quit /tmp/mpv-socket3

super + ctrl + y
	dmpv general/videos/ /tmp/mpv-socket2 /tmp/mpv-socket --fs ls

super + ctrl + u
	dmpv general/music/ /tmp/mpv-socket /tmp/mpv-socket2 --no-video

super + alt + {y,u}
	rmpv general/{videos,music}/ 150

super + ctrl + {i,p}
	hmpv {back,forward} /tmp/mpv-socket

super + alt + {i,p}
	hmpv {prev,next} /tmp/mpv-socket

super + {ctrl,alt} + o
	hmpv {toggle,quit} /tmp/mpv-socket

super + {minus,equal}
	hvol 3%{-,+}

super + shift {minus,equal}
	hvol 6%{-,+}

{XF86AudioLowerVolume, XF86AudioRaiseVolume}
	hvol 3%{-,+}

shift + {XF86AudioLowerVolume, XF86AudioRaiseVolume}
	hvol 6%{-,+}

super + y
	st -e sh -c 'autowifi && cu undock'

super + shift + y
	st -e sh -c 'wifioff && cu dock'

super + ctrl + {j,k}
	for void in $(seq 3); do xdotool click {5,4}; done

Menu
	xdotool click 2

