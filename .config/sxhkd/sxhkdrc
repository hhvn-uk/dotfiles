super + {shift, ctrl, alt, shift + ctrl, shift + alt} + v
	dinput "Are you sure?" "{sudo zzz, sudo halt, sudo reboot, sudo ZZZ, pkill dwm}"

super + Return
	st

super + a
	addrbook

super + shift + a
	mailselect

super + b
	tabbed -c vimb -e

super + shift + b
	dmpvurl /tmp/mpv-socket /tmp/mpv-socket2

super + shift + l
	tabbed -c st -w > /tmp/txid

super + n
	st -e mpvrcp /tmp/mpv-socket

super + shift + n 
	st -e neomutt

super + r
	st -e sh -c 'sfeed_update $HOME/.config/sfeed/config && SFEED_PLUMBER=urlhand2 sfeed_curses $HOME/.cache/sfeed/*'

super + x
	sxhkdrestart

super + ctrl + b
	queuevid

super + c
	st -e slock_mod /tmp/mpv-socket /tmp/mpv-socket2 /tmp/mpv-socket3

super + shift + c
	slock

super + d
	dmenu_run

super + e
	st -e tmux

super + ctrl + e
	rfeh ~/images/wallpapers/ auto right

super + m
	dmon

super + ctrl + q
	rfeh ~/images/wallpapers/ auto left

super + s
	dpass

super + shift + s
	maim -s | hlpaste "png"

super + w
	rfeh ~/images/wallpapers/

super + ctrl + w
	wallblur

super + alt + w
	fehffile ~/.cache/rfeh/prev

super + shift + w
	colour-wall

super + ctrl + t
	chradio

super + akt + t
	hmpv quit /tmp/mpv-socket3

super + ctrl + y
	dmpv general/videos/ /tmp/mpv-socket2 /tmp/mpv-socket --fs ls

super + ctrl + u
	dmpv general/music/ /tmp/mpv-socket /tmp/mpv-socket2 --no-video

super + alt + {y,u}
	rmpv general/{videos,music}/ 150

super + ctrl + {i,p}
	hmpv {back,forward} /tmp/mpv-socket

super + alt + {i,p}
	hmpv {prev,next} /tmp/mpv-socket

super + {ctrl,alt} + o
	hmpv {toggle,quit} /tmp/mpv-socket

super + {minus,equal}
	hvol 3%{-,+}

super + shift {minus,equal}
	hvol 6%{-,+}
