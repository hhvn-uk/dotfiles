#!/bin/sh

grep -E 'A: [0-9]|title: ' | tr '\n' '\t' | sed 's/A: [0-9]/\n&/g' | grep '^A: ' | awk '
	BEGIN {
		start="00:00"
	}

	// {
		printf("%s\t%s\t", start, $2) 
		start=$2
		$1=""
		$2=""
		$3=""
		$4=""
		$5=""
		$6=""
		$7=""
		$8=""
		print
	}' | sed -E 's/        //;s/	([^	]*)	([^-]*)-(.*)/	\1	\3	\2/;s/	 /	/g'
